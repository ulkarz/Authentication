<%- include ../partials/header.ejs %>

    <%- include ../partials/main_nav.ejs %>

        <!-- Content for the Add page -->
        <div class="container">
            <div class="row">
                <div class="offset-md-3 col-md-6"></div>
                <h1>
                    <% title %>
                </h1>

                <form class="form" method="POST">
                    <div class="form-group">
                        <label for="nameTextField">Name</label>
                        <input type="text" class="form-control" id="nameTextField" placeholder="Enter the name" name="name" required>
                    </div>

                    <div class="form-group">
                        <label for="numberTextField">Number</label>
                        <input type="number" class="form-control" id="numberTextField" placeholder="Enter the number" name="number" required>
                    </div>

                    <div class="form-group">
                        <label for="emailTextField">Email</label>
                        <input type="text" class="form-control" id="emailTextField" placeholder="Enter the email" name="email" required>
                    </div>

                </form>
            </div>
        </div>


        <%- include ../partials/bottom.ejs %>

            <%- include ../partials/footer.ejs %>